<Window x:Class="projekt_bazy.DelUserSure"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projekt_bazy"
        mc:Ignorable="d"
        Title="DelUserSure" Height="150" Width="350">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Label x:Name="label" Content="Czy na pewno chcesz usunąć użytkownika " HorizontalAlignment="Center" Height="33" Grid.Row="0" VerticalAlignment="Center" Width="322"/>
        <Button x:Name="ButtonYes" Content="Tak" HorizontalAlignment="Right" Grid.Row="1" VerticalAlignment="Center" Width="130" Margin="0,20,10,19" Click="ButtonYes_Click"/>
        <Button x:Name="ButtonNo" Content="Nie" HorizontalAlignment="Left" Grid.Row="1" VerticalAlignment="Center" Width="130" Margin="10,20,10,19" Click="ButtonNo_Click"/>

    </Grid>
</Window>
